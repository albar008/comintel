    },
    "bgImgUrlLarge": {
      "type": "string"
    },
    "bgImgUrlThumb": {
      "type": "string"
    }
  },
  "viewScriptModule": "file:./view.js",
  "supports": {
    "renaming": false,
    "color": {
      "text": false
    }
  }
}