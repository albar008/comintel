{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "comintel-business/general-content-header",
  "title": "General Content Header Block",
  "editorScript": "file:./index.js",
  "description": "General Content Header block for Comintel Business theme",
  "render": "file:./render.php",
  "category": "comintel-business",
  "attributes": {
    "title": {
      "type": "string",
      "default": "Title Here"
    },
    "subtitle": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "isCentered": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "customClassName": false,
    "renaming": false,
    "color": {
      "text": true,
      "background": false
    }
  }
}